{
  "name": "pagseguro/pagseguro-php-sdk",
  "description": "Biblioteca de integração com o PagSeguro",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "PagSeguro"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0",
    "ext-curl": "*",
    "lib-curl": "*",
    "lib-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "PagSeguro\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PagSeguro\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0.0-dev"
    }
  }
}
